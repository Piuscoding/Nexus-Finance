                  <div class="row mb-5 g-3">
                     <div class="col-md-12">
                        <!-- Card -->
                        <div class="card border border-dashed mb-3">
                           <!-- <div class="card-header p-3 border-bottom">
                              <form class="mx-0">
                                 <input type="search" id="searchInput" class="form-control rounded-2 border border-dashed bg-light-subtle border-primary text-primary" placeholder="Search loans...">
                              </form>
                           </div> -->
                           <div class="card-body p-0">
                            <%if(user.upgrades != ''){%>
                              <div class="table-responsive">
                                 <table class="table table-borderless" id="vertex-table" style="width: 100%;">
                                    <thead class="bg-light-subtle border-bottom">
                                       <tr>
                                          <th scope="col">Loan ID</th>
                                          <th scope="col">Loan Status</th>
                                          <th scope="col">Amount</th>
                                          <th scope="col">Date Initialized</th>
                                          <th scope="col">Category</th>
                                          <th scope="col">Duration</th>
                                          <th scope="col">Interest(%)</th>
                                          <th scope="col">Repayment(%)</th>
                                          <th scope="col">Payment Status</th>
                                          <th scope="col">Preview Details</th>
                                                                                 </tr>
                                    </thead>
                                    <tbody>
                                        <% if(user.upgrades.length > 0 ){ %>
                                            <%  user.upgrades.forEach(loan => {  %>
                                                                                    <tr>
                                                <!-- Loan ID -->
                                                <td scope="row">
                                                   <span class="fw-bold text-dark">
                                                      <%=loan._id%>                                                  </span>
                                                </td>

                                                <!-- Loan Status -->
                                                <td>
                                                   <span class="badge fw-light bg-light-subtle border rounded-1 text-warning border-warning border-dashed">
                                                    <%=loan.status%>                                              </span>
                                                </td>

                                                <!-- Loan Amount -->
                                                <td>
                                                    $<%=loan.loan_amount%>                                           </td>

                                                <!-- Date Initialized -->
                                                <td>
                                                   <%=loan.createdAt%>                                             </td>

                                                <!-- Category -->
                                                <td>
                                                   <button type="button" class="badge fw-light bg-light-subtle text-primary border-primary rounded-1 border-dashed border">
                                                    <%=loan.loan_category%>                                                  </button>
                                                </td>

                                                <!-- Duration -->
                                                <td>
                                                   <span class="fw-bold">
                                                    <%=loan.loan_duration%>                                                  </span>
                                                </td>

                                                <!-- Interest -->
                                                <td>
                                                   <button type="button" class="badge fw-light bg-light-subtle text-success border-success rounded-1 border-dashed border">
                                                    <%=loan.loan_interest_amount%> 
                                                   </button>
                                                </td>

                                                <!-- Repayment -->
                                                <td>
                                                    <%=loan.repayment%>                                                 </td>

                                                <!-- Payment Status -->
                                                <td>
                                                   <span class="badge fw-light bg-light-subtle border rounded-1 text-warning border-warning border-dashed">
                                                    <%=loan.payStatus%> 
                                                   </span>
                                                </td>

                                                <!-- Preview Link -->
                                                <td>
                                                   <a href="javascript:void(0);" data-bs-toggle="modal" data-bs-target="#loanDetailsPreviewModal" data-bs-loan-payload='{&quot;loan_id&quot;:&quot;#<%=loan._id%>&quot;,&quot;status&quot;:&quot;<%=loan.status%>&quot;,&quot;amount&quot;:&quot;<%=loan.loan_amount%>&quot;,&quot;date_initialized&quot;:&quot;<%=loan.createdAt%>&quot;,&quot;category&quot;:&quot;<%=loan.loan_category%>&quot;,&quot;duration&quot;:&quot;<%=loan.loan_duration%>&quot;,&quot;interest&quot;:&quot;<%=loan.loan_interest_amount%>&quot;,&quot;repayment&quot;:&quot;<%=loan.loan_interest_percentage%>&quot;,&quot;reason&quot;:&quot;<%=loan.loan_reason%>&quot;,&quot;payment_status&quot;:&quot;<%=loan.payStatus%> &quot;}' class="badge fw-light bg-light-subtle text-primary border-primary rounded-1 border-dashed border">
                                                      Preview Receipt
                                                   </a>
                                                </td>

                                                <!-- Manage Loan -->
                                                                                             </tr>
                                                       <%  }) %>
                                                       <%  } %>
                                                                              </tbody>
                                 </table>
                                 <% }else{%>
                                    <h3 class="text-center text-secondary mt-5">No Loan found!</h3>
                                    <% } %>
                              </div>
                           </div>
